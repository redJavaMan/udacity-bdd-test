{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:playwright/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  // "parserOptions": {
  //     "project": [
  //         "./tsconfig.json"
  //     ]
  //},
  "plugins": ["@typescript-eslint"],
  "ignorePatterns": ["node_modules/", "playwright-report"],
  "rules": {
    "expect-expect": "off",
    "playwright/expect-expect": "off"
  }
}
